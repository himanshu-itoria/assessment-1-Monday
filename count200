#!/bin/bash

log_file="/web-server(5).log"  # Replace with the actual path to your log file
http_200_count=0

while IFS= read -r line; do
    # Extract the status code from the log line
    status_code=$(echo "$line" | awk '{print $9}')

    # Check if the status code is 200
    if [ "$status_code" = "200" ]; then
        ((http_200_count++))
    fi
done < "$log_file"

echo "Number of HTTP 200 errors: $http_200_count"

